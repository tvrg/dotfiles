{
  "coc.preferences.formatOnSaveFiletypes": ["rust"],
  "suggest.noselect": false,
  "suggest.removeDuplicateItems": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  },
  "go.goplsOptions": {
    "completeUnimported": true
  }
}
